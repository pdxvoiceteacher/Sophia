{
  "$id": "ucc.public_signals.vote_proof_envelope.v1",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "aad_sha256": {
      "pattern": "^[0-9a-f]{64}$",
      "type": "string"
    },
    "ballot_id": {
      "minLength": 1,
      "type": "string"
    },
    "choice_hash": {
      "pattern": "^[0-9a-f]{64}$",
      "type": "string"
    },
    "ciphertext_sha256": {
      "pattern": "^[0-9a-f]{64}$",
      "type": "string"
    },
    "manifest_id": {
      "minLength": 1,
      "type": "string"
    },
    "nullifier_sha256": {
      "pattern": "^[0-9a-f]{64}$",
      "type": "string"
    },
    "public_inputs": {
      "additionalProperties": false,
      "properties": {
        "format": {
          "const": "snarkjs.public_inputs.v1"
        },
        "order": {
          "items": {
            "minLength": 1,
            "type": "string"
          },
          "minItems": 1,
          "type": "array"
        },
        "values": {
          "items": {
            "type": "string"
          },
          "minItems": 1,
          "type": "array"
        },
        "version": {
          "const": 1
        }
      },
      "required": [
        "format",
        "version",
        "order",
        "values"
      ],
      "type": "object"
    }
  },
  "required": [
    "manifest_id",
    "ballot_id",
    "nullifier_sha256",
    "ciphertext_sha256",
    "aad_sha256",
    "choice_hash",
    "public_inputs"
  ],
  "type": "object"
}
